;;; -*- mode: beancount -*-
;;; Coinbean - Cryptocurrency Finance Ledger
;;; Track your crypto assets across multiple exchanges and blockchains

;; Options and plugins
option "title" "Coinbean - Crypto Portfolio Tracker"
option "operating_currency" "USD"

;; Tolerance for rounding errors
;; Crypto can have many decimal places, so we set higher precision
option "inferred_tolerance_default" "USD:0.01"
option "inferred_tolerance_default" "BTC:0.00000001"  ;; 1 satoshi
option "inferred_tolerance_default" "ETH:0.000001"    ;; 1 gwei
option "inferred_tolerance_default" "SOL:0.000001"
option "inferred_tolerance_default" "*:0.0001"        ;; Default for other cryptos

;; Plugins
plugin "beancount.plugins.implicit_prices"

